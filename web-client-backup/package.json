    "@mui/x-date-pickers": "^6.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.14.195",
    "@types/node": "18.15.10",
    "@types/react": "18.0.30",
    "@types/react-datepicker": "^4.11.2",
    "@types/react-dom": "18.0.11",
    "@vercel/analytics": "^1.0.1",
    "axios": "^1.3.4",
    "caniuse-lite": "^1.0.30001741",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
